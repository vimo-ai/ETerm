{
    "id": "com.eterm.vlaude",
    "name": "Vlaude Remote",
    "version": "1.0.0",
    "minHostVersion": "0.0.1-beta.1",
    "sdkVersion": "0.0.1-beta.1",
    "runMode": "main",
    "loadPriority": "immediate",
    "dependencies": [
        {"id": "com.eterm.aicli", "minVersion": "1.0.0"}
    ],
    "capabilities": ["socket.client"],
    "principalClass": "VlaudePlugin",
    "sidebarTabs": [
        {
            "id": "vlaude-settings",
            "title": "Remote",
            "icon": "iphone.and.arrow.forward",
            "viewClass": "VlaudeSettingsView",
            "order": 10
        }
    ],
    "subscribes": [
        "aicli.sessionStart",
        "aicli.promptSubmit",
        "aicli.responseComplete",
        "aicli.sessionEnd",
        "aicli.permissionRequest",
        "aicli.toolUse",
        "terminal.didClose"
    ],
    "tabSlots": [
        {
            "id": "vlaude-mobile-viewing",
            "position": "trailing"
        }
    ],
    "emits": []
}
